{
    "kind": "collectionType",
    "collectionName": "brands",
    "info": {
        "singularName": "brand",
        "pluralName": "brands",
        "displayName": "Brand"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "brand_name": {
            "type": "string"
        },
        "logo": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "stores": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::store.store",
            "mappedBy": "brand"
        }
    }
}
